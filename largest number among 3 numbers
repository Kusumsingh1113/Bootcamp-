#include<stdio.h>
void main()
{
    int a,b,c;
    printf("Enter a : ");
    scanf("%d",&a);
    printf("Enter b : ");
    scanf("%d",&b);
    printf("Enter c : ");
    scanf("%d",&c);

    if(a>b)
    {
        if(a>c)
        {
            printf("%d is greatest",a);
        }
    }
    else if(b>a)
    {
        if(b>c)
        {
            printf("b is greatest");
        }
    }
    else
    {
        printf("c is greatest");
    }
}
    
